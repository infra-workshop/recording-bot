{
  "name": "recorder-tab-cap-support",
  "description": "a plugin to support tab capture",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content_script.js"]
  }],
  "web_accessible_resources": [
    "web_script.js"
  ],
  "permissions": [
    "<all_urls>",
    "tabCapture"
  ]
}